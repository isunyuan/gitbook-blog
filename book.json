{
  "title": "FE Notes",
  "author": "SunYuan",
  "description": "Welcome to my blog",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "styles": {
    "website": "styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "plugins": [
    "-sharing",
    "fontsettings",
    "-lunr",
    "-search",
    "search-pro",
    "expandable-chapters-small",
    "sidebar-style",
    "github-buttons",
    "favicon",
    "code",
    "popup",
    "back-to-top-button",
    "tbfed-pagefooter",
    "edit-link-plus"
  ],
  "pluginsConfig": {
    "edit-link-plus": {
      "base": "https://github.com/isunyuan/blog/edit/master",
      "label": "编辑本页"
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "isunyuan",
          "repo": "blog",
          "type": "star",
          "size": "small",
          "count": true
        }
      ]
    },
    "sidebar-style": {
      "title": "FE Notes",
      "author": "email@isunyuan.com"
    },
    "favicon": {
      "shortcut": "source/images/favicon.ico",
      "bookmark": "source/images/favicon.ico",
      "appleTouch": "source/images/apple-touch-icon.png",
      "appleTouchMore": {
        "120x120": "source/images/apple-touch-icon.png",
        "180x180": "source/images/apple-touch-icon.png"
      }
    },
    "tbfed-pagefooter": {
      "copyright": "人是挂在自己编织的意义之网上的动物。",
      "modify_label": "更新时间：",
      "modify_format": "YYYY-MM-DD"
    }
  }
}
